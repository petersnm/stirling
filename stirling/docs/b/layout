    .obj
        # Every object in the MUD
        .__init__
            from stirling.obj.object import MasterObject
            from stirling.obj.living.living import Living
            from stirling.obj.living.sapient import Sapient
        .living
            # Things that move on their own
            .living
                # The file for living things
                class Living(MasterObject)
                .sapient
                    # Things that can do complex things (speak, hold citizenship, etc.)
                    class Sapient(Living)
        .room
            # base inheritance for all rooms
        .object
            # MasterObject
            class MasterObject()
            class Inventory()
        .spec
            # Speeecial things
            .player
                # Handle inputs from terminal
                class Player(Sapient)
            .daemon
                # Inheritable for stirling.daemon
                class Daemon(MasterObject)
